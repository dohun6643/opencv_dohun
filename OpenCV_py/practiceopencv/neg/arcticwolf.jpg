	function rsMenu(schoolList) {
    	head.ready("rsMainMenu", function(){
        
            var socialIcons = [
                ['Facebook', 'https://www.facebook.com/HenryCountySchools/'],
                ['Twitter', 'https://twitter.com/henrycountyboe'],
                ['YouTube', 'https://www.youtube.com/channel/UCLZZRhc4bbFmq9HpTzccerQ'],
                ['Instagram', ''],
                ['Google Plus', ''],
                ['Pinterest', ''],
                ['Vimeo', ''],
                ['Flickr', ''],
                ['Linked In', '']
            ];

            var mobileSocialLinks = '';


            for(var i = 0; i < socialIcons.length; i++) {
                if(socialIcons[i][1] != ''){
                    mobileSocialLinks += "3,"+socialIcons[i][0]+","+socialIcons[i][1]+", _blank;";
            	}
           	}
            
            var mobileSchools = '';
            
             for(var i in schoolList) {
                        if(schoolList[i].schoolVisible.toLowerCase() == 'true') {
                            mobileSchools += '1,' + schoolList[i].schoolName + ',' + schoolList[i].schoolUrl + ', _blank;';
                        }
                    }

        
        
        
        
        
        
            // RESPONSIVE MAIN MENU
            $("body").csRsMenu({
                "breakPoint"            : 768, // SYSTEM BREAK POINTS - 768, 640, 480, 320
                "slideDirection"        : "left-to-right", // OPTIONS - left-to-right, right-to-left   
                "primaryColor"          : "#1A1A1A", // DEFAULT - #1A1A1A
                "secondaryColor"        : "#0C0C0C", // DEFAULT - #0C0C0C
                "accentColor"           : "#BABABA", // DEFAULT - #BABABA
                "menuButtonParent"      : "#gb-mystart-left",
                "showMenuBtnText"       : "yes",
                "menuBtnText"           : "MENU",
                "showSchools"           : "no",
                "showTranslate"         : "yes",
                "translateId"           : "none",
                "showAccount"           : "yes",
                "extraMenuOptions"      : "Schools; Audiences; Social Media", // EXTRA MENU HEADING NAMES SEPARATED BY A ;
                "extraMenuItems"        : mobileSchools+"2, Parents, https://schoolwires.henry.k12.ga.us/parents, _self; 2, Students, https://schoolwires.henry.k12.ga.us/students, _self; 2, Community, https://schoolwires.henry.k12.ga.us/community, _self; 2, Employees, https://schoolwires.henry.k12.ga.us/employees, _self;"+mobileSocialLinks, // EXTRA MEmobileSocialLinks, // EXTRA MENU BUTTONS - STRUCTURE IS: MENU-OPTION-NUMBER, ITEM-NAME, ITEM-URL, ITEM-TARGET;
                "siteID"                : "1",
                "allLoaded"             : function(){ }
            });
		});
    }


