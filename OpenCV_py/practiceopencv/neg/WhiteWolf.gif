<script>
if(getCookie('mm-castor-logged-in')){
    $(document).ready(function() {
        userProfile.loadUserProfile();
    });
    toggleVisibility('login-box', 'no-login-box');
    toggleVisibility('login-btn', 'logout-btn');
    toggleVisibility('mobile-login-btn', 'mobile-logout-btn');
    toggleVisibility('login-mobile-btn', 'redirect-mobile-btn');
    toggleVisibility('mein-email-login', 'mein-email-redirect');
    toggleVisibility('footer-email-login', 'footer-email-redirect');
    $('#login-info-mobile').toggle();
    window.addEventListener('userProfileUpdated', function (e) {
        if (window.location.href.indexOf('login=true') > -1 && window.location.href.indexOf('redirect=') <= -1) {
            var data = userProfile.getUserProfile();
            if(data.additionalInfo) {
                var popupAppear = $.grep(data.additionalInfo, function(e){ return e.key == "popupAppear"; })[0];
                if ((typeof popupAppear != 'undefined') && (popupAppear.value === 'false')) {
                    localStorage.setItem('popUpAppear', "false");
                } else {
                    localStorage.setItem('popUpAppear', "true");
                }
                var userLoggedOut = $.grep(data.additionalInfo, function(e){ return e.key == "userLoggedOut"; })[0];
                userProfile.saveAdditionalInfoParam("userLoggedOut","false", true);
                // if ((typeof userLoggedOut != 'undefined') && (userLoggedOut.value === 'false')) {
                //     var redirect = window.location.pathname + window.location.search.replace("login=true", "");
                //     var url = '/portal/page/logout/hint?redirect=' + encodeURIComponent(redirect);
                //     window.location.replace(window.location.origin.concat(url));
                // } else {
                    var redirect = window.location.pathname + window.location.search.replace("login=true", "");
                    window.history.replaceState(null, null, redirect);
                    userProfile.dispatchEvent('userProfileUpdated-noRedirect');
                    userProfile.dispatchEvent('userProfileLoadComplete');
               // }
            } else {
                userProfile.dispatchEvent('userProfileUpdated-noRedirect');
                userProfile.dispatchEvent('userProfileLoadComplete');
            }
        } else {
            userProfile.dispatchEvent('userProfileUpdated-noRedirect');
            userProfile.dispatchEvent('userProfileLoadComplete');
        }
    }, false);
} else {
    userProfile.dispatchEvent('userProfileLoadComplete');
}
</script>
